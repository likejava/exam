<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cmcc.ict.exam.po.Result" table="tb_result"><!-- 每个class对应一个持久化对象 -->
		<id name="id" ><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<generator class="identity"></generator>
		</id>
		<property name="examId"></property>
		<property name="subjectId"></property>
		<property name="selectOption"></property>
		<property name="user"></property>
		<property name="score"></property>
	</class>	
</hibernate-mapping>